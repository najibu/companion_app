<li id="<%= user.id %>">
  <div class="img"><%= image_tag user.image(:medium), height: '370', width: '370' %></div>
  <div class="user-name pull-left">
    <%= link_to profile_user_path(user.id) do %><b><%= name(user) %></b>, <%= age(user) %></a><% end %>
  </div>
  <div class="user-location text-right"><i class="fa fa-map-marker"></i> <%= location(user) %></div>
  <div class="like"></div>
  <div class="dislike"></div>
  <%= link_to "", create_friendships_path(friend_id: user.id), id: "user_#{user.id}", method: :post, :remote => true %>
</li>

